  "devDependencies": {
    "electron-packager": "^8.5.2",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0"
  },
  "dependencies": {
    "electron": "^1.6.1",
    "electron-config": "^0.2.1",
    "electron-devtools-installer": "^2.1.0",
    "electron-reload": "^1.1.0",
    "exif-parser": "^0.1.9",
    "junk": "^2.0.0",
    "vue": "^2.1.10"
  }
